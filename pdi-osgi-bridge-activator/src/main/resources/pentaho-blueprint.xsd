  </xsd:complexType>

  <xsd:element name="di-plugin-mapping" type="TdiPluginMapping">

    <xsd:annotation>
      <xsd:documentation>
        <![CDATA[
Marks the enclosing bean as a class containing Annotations describing a PDI Plugin Type
]]>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>


  <xsd:complexType name="TdiPluginSupplementalMappings">
    <xsd:annotation>
